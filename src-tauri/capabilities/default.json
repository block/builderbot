{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "core:window:allow-start-dragging",
    "dialog:default",
    "clipboard-manager:allow-write-text",
    "window-state:default",
    "mcp-bridge:default"
  ]
}
